<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-
//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate
-mapping-3.0.dtd">



<hibernate-mapping package="modelo">
	<class name="EmpresaDeMudanzas" table="EMPRESA_DE_MUDANZAS">
		<id name"id" column="ID_EMPRESA_DE_MUDANZAS">
			<generator class="native"/>
		</id>
		<property name="cuit" type="string" />
		<property name="razonSocial" type="string" />
		<list name="pendientes" cascade="all">
	         <key column="ID_MUDANZA"/>
	         <list-index column="idx"/>
	         <many-to-one class="Mudanza"/>
      	</list>
      	<list name="pendientes" cascade="all">
	         <key column="ID_MUDANZA"/>
	         <list-index column="idx"/>
	         <many-to-one class="Mudanza"/>
      	</list>
	</class>
</hibernate-mapping>