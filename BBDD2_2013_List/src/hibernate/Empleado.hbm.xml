<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-
//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate
-mapping-3.0.dtd">
<hibernate-mapping package="modelo">
	<class name="Empleado" table="EMPLEADO">
		<id name"idEmpleado" column="ID_EMPLEADO">
			<generator class="native"/>
		</id>
		<property name="nombreCompeto" type="string"/>
		<property name="cuil" type="string"/>
		<property name="fechaNaciemiento" type="date"/>		
	</class>
</hibernate-mapping>